{
    "name": "portfolio",
    "version": "1.0.0",
    "private": true,
    "description": "Portfolio",
    "author": "Mike-cheek",
    "homepage": "https://mike-cheek.github.io/portfolio",
    "keywords": [
        "portfolio"
    ],
    "license": "UNLICENSED",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "export": "next export",
        "deploy": "next build && gh-pages -d public",
        "deploygh": "next build && next export && git add out/ && git commit -m \"Deploy gh-pages\" && git subtree push --prefix out origin gh-pages",
        "push": "git add . && git commit && git push",
        "prepare": "husky install",
        "format": "prettier --write .",
        "lint": "next lint",
        "lint-init": "eslint --init",
        "prettify": "pretty-quick",
        "type-check": "tsc",
        "check-types": "tsc --pretty --noEmit",
        "check-format": "prettier --check .",
        "check-lint": "next lint --fix",
        "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build"
    },
    "dependencies": {
        "next": "^12.1.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-helmet": "^6.1.0",
        "sass": "^1.49.9",
        "stable": "^0.1.8"
    },
    "devDependencies": {
        "@next/eslint-plugin-next": "^12.1.1",
        "@types/node": "^16.11.12",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@types/react-helmet": "^6.1.4",
        "@types/styled-components": "^5.1.19",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "eslint": "^8.12.0",
        "eslint-plugin-react": "^7.29.4",
        "file-loader": "^6.2.0",
        "husky": "^7.0.0",
        "prettier": "2.6.1",
        "pretty-quick": "^3.1.3",
        "typescript": "^4.6.2",
        "uuid": "^8.3.2"
    },
    "peerDependencies": {
        "@babel/core": "^7.12.3",
        "babel-eslint": "^10.1.0",
        "typescript": "^4.5.5",
        "webpack": "^5.0.0"
    }
}